{
  "name": "lazy-tree",
  "version": "1.0.1",
  "description": "An implementation for lazily reducing values over trees",
  "main": "src/index.js",
  "scripts": {
    "test": "xo && nyc --check-coverage --lines 100 --reporter=text ava"
  },
  "keywords": [],
  "author": "Chet Corcos <ccorcos@gmail.com> (http://www.chetcorcos.com/)",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "nyc": "^8.1.0",
    "xo": "^0.16.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "guard-for-in": 0,
      "no-use-before-define": 0,
      "camelcase": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "operator-linebreak": [
        2,
        "before"
      ]
    }
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
